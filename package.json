{
  "name": "smarttimetable",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "homepage": "https://first-dev.github.io/smart-timetable",
  "scripts": {
    "lint": "eslint . --ext .ts,.tsx",
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "electron": "expo-electron start",
    "electron:build": "yarn electron-webpack && yarn electron-builder --dir -c.compression=store -c.mac.identity=null",
    "deploy": "gh-pages -d web-build",
    "predeploy": "expo build:web && expo publish"
  },
  "dependencies": {
    "@expo/vector-icons": "^12.0.5",
    "@react-native-async-storage/async-storage": "~1.15.0",
    "@react-native-community/datetimepicker": "4.0.0",
    "@react-navigation/drawer": "^6.3.1",
    "@react-navigation/native": "^6.0.8",
    "@react-navigation/native-stack": "^6.5.0",
    "date-fns": "^2.28.0",
    "easy-peasy": "^5.0.4",
    "expo": "~44.0.0",
    "expo-status-bar": "~1.2.0",
    "expo-updates": "~0.11.6",
    "lodash": "^4.17.21",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-native": "0.64.3",
    "react-native-calendars": "^1.1279.0",
    "react-native-circular-progress": "^1.3.7",
    "react-native-console-time-polyfill": "^1.2.3",
    "react-native-gesture-handler": "~2.1.0",
    "react-native-paper": "^4.11.2",
    "react-native-reanimated": "~2.3.1",
    "react-native-safe-area-context": "3.3.2",
    "react-native-screens": "~3.10.1",
    "react-native-svg": "12.1.1",
    "react-native-web": "0.17.1",
    "react-navigation-header-buttons": "^9.0.1",
    "recoil": "^0.6.1",
    "recoil-persist": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@expo/electron-adapter": "^0.0.55",
    "@expo/webpack-config": "^0.16.18",
    "@types/lodash": "^4.14.179",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.64.12",
    "@types/recoilize": "^0.8.0",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "babel-loader": "^8.2.3",
    "electron": "^6.0.12",
    "electron-builder": "^22.14.13",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-react": "^7.29.2",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-react-native": "^4.0.0",
    "gh-pages": "^3.2.3",
    "prettier": "^2.5.1",
    "recoilize": "^2.0.1",
    "typescript": "~4.3.5"
  },
  "private": true,
  "build": {
    "extraMetadata": {
      "main": "main.js"
    },
    "files": [
      {
        "from": "dist/main/",
        "to": "./",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "dist/renderer",
        "to": "./",
        "filter": [
          "**/*"
        ]
      },
      "package.json",
      "**/node_modules/**/*"
    ]
  }
}
